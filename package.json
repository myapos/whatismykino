    "file-loader": "^3.0.1",
    "graphql": "^14.0.2",
    "graphql-anywhere": "^4.1.23",
    "graphql-tag": "^2.10.0",
    "html-webpack-plugin": "^3.2.0",
    "httpster": "^1.0.4",
    "husky": "^1.3.1",
    "imports-loader": "^0.8.0",
    "jest": "^25.0.0",
    "jquery": "^3.3.1",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.11.0",
    "lint-staged": "^7.2.2",
    "mini-css-extract-plugin": "^0.4.3",
