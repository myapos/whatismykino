{
  "name": "what-is-my-kino-number",
  "description": "A web app that provides basic tools for data processing on Kino lottery",
  "version": "0.0.11",
  "author": "Myron Apostolakis",
  "scripts": {
    "serve": "httpster -p 2222 -d ./dist",
    "devOld": "PRODUCTION=false USAGE=dev webpack-dev-server --mode development --hot",
    "dev": "PRODUCTION=false USAGE=dev webpack-dev-server --config webpack.dev.js --mode development --hot",
    "devPython": "USE_PYTHON_SERVER=true PRODUCTION=false USAGE=dev webpack-dev-server --config webpack.dev.js --mode development --hot",
    "startDev": "concurrently \"npm run dev\" \"npm run server\"",
    "start": "USE_PYTHON_SERVER=true concurrently \"npm run serve\" \"npm run serverPy\"",
    "start_": "USE_PYTHON_SERVER=true npm run serverPy || npm run serve",
    "startClientAndPython": "USE_PYTHON_SERVER=true concurrently \"npm run devPython\" \"npm run serverPy\"",
    "build": "USAGE=prod PRODUCTION=true USE_PYTHON_SERVER=true webpack --mode production --config webpack.prod.js",
    "build_": "USAGE=prod PRODUCTION=true webpack --mode production --config webpack.prod.js",
    "buildOld": "USAGE=prod webpack --mode production --config webpack.config.production.js",
    "test": "jest",
    "server": "nodemon ./server/server.js",
    "serverPy": "FLASK_APP=python/server.py flask run",
    "precommit": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "post-merge": "node ./buildVersion.js && git commit -am 'update version' && git push"
    }
  },
  "dependencies": {
    "30-seconds-of-code": "^1.2.3",
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.0.0",
    "babel-loader": "^8.0.0",
    "bootstrap": "^4.2.1",
    "bootstrap-daterangepicker": "^3.0.5",
    "chalk": "^2.3.0",
    "clean-webpack-plugin": "^0.1.19",
    "core-js": "^2.6.11",
    "cors": "^2.8.5",
    "css-loader": "^1.0.0",
    "dataloader": "^1.4.0",
    "es6-promise": "^4.2.5",
